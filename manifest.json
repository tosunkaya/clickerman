{
  "name": "clickerman",
  "version": "1.0",
  "description": "Autoclicker for YouTube etc.",
  "icons": { "16": "./pics/cursor16.png",
    "48": "./pics/cursor.png",
    "128": "./pics/cursor128.png" 
    },

 "page_action": {
 "default_icon": "./pics/cursor.png"
 },

"content_scripts": [
 {
 "matches": [
   "https://*.youtube.com/*", 
   "https://*.google.com/*", 
   "https://*.twitch.tv/*" 
  ],
 "js": [
   "dark.js", 
   "click.js"
  ],
 "all_frames": true
 }
 ],
  "manifest_version": 2
}
